    <build>
        <nagEmailAddress>jaster@seis.sc.edu</nagEmailAddress>
        <sourceDirectory>src</sourceDirectory>
        <unitTestSourceDirectory>tests/unit</unitTestSourceDirectory>
        <unitTest>
            <includes>
                <include>edu/**/*Test.java</include>
            </includes>
            <resources>
                <resource>
                    <directory>tests/database/edu/sc/seis/fissuresUtil/database/event</directory>
                    <targetPath>edu/sc/seis/fissuresUtil/database/event/</targetPath>
                    <includes>
                        <include>*.props</include>
                    </includes>
                    <filtering>false</filtering>
                </resource>
                <resource>
                    <directory>tests/unit/edu/sc/seis/fissuresUtil/bag</directory>
                    <targetPath>edu/sc/seis/fissuresUtil/bag/</targetPath>
                    <includes>
                        <include>03.141.18.40.32.0551.6.m</include>
                    </includes>
                    <filtering>false</filtering>
                </resource>
            </resources>
        </unitTest>
        <resources>
            <resource>
                <directory>src/edu/sc/seis/fissuresUtil/database/util</directory>
                <targetPath>edu/sc/seis/fissuresUtil/database/util</targetPath>
                <includes>
                    <include>SQLLoader.prop</include>
                </includes>
                <filtering>false</filtering>
            </resource>
            <resource>
                <directory>src/edu/sc/seis/fissuresUtil/database/props</directory>
                <targetPath>edu/sc/seis/fissuresUtil/database/props</targetPath>
                <includes>
                    <include>**/*.props</include>
                </includes>
                <filtering>false</filtering>
            </resource>
            <resource>
                <directory>src/edu/sc/seis/fissuresUtil/chooser</directory>
                <targetPath>edu/sc/seis/fissuresUtil/chooser</targetPath>
                <includes>
                    <include>ChannelChooser.properties</include>
                </includes>
                <filtering>false</filtering>
            </resource>
            <resource>
                <directory>src/edu/sc/seis/fissuresUtil/data</directory>
                <targetPath>edu/sc/seis/fissuresUtil/data</targetPath>
                <includes>
                    <include>**</include>
                </includes>
                <filtering>false</filtering>
            </resource>
            <resource>
                <directory>src/edu/sc/seis/fissuresUtil/display</directory>
                <targetPath>edu/sc/seis/fissuresUtil/display</targetPath>
                <includes>
                    <include>*.prop</include>
                </includes>
                <filtering>false</filtering>
            </resource>
            <resource>
                <directory>src/edu/sc/seis/fissuresUtil/map</directory>
