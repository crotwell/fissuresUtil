<?xml version="1.0"?>
<seismogramDisplay>
    <dontDrawNamedDrawableNames/>
    <flagColors>
        <color>
            <red>233</red>
            <green>166</green>
            <blue>49</blue>
            <alpha>255</alpha>
        </color>
    </flagColors>
    <traceColors>
        <color>
            <red>80</red>
            <green>93</green>
            <blue>161</blue>
            <alpha>255</alpha>
        </color>
    </traceColors>
    <borderBackground>
        <red>204</red>
        <green>204</green>
        <blue>204</blue>
        <alpha>255</alpha>
    </borderBackground>
    <borderColor>
        <red>12</red>
        <green>100</green>
        <blue>76</blue>
        <alpha>255</alpha>
    </borderColor>
    <border>
        <type>time</type>
        <position>bottom</position>
        <titleFont>
            <size>18</size>
            <name>MS-Trebuchet</name>
            <style>bold</style>
        </titleFont>
    </border>
    <border>
        <type>title</type>
        <position>top</position>
        <title>
            <text>Earth's Motion vs. Time</text>
            <font>
                <size>18</size>
                <name>MS-Trebuchet</name>
                <style>bold</style>
            </font>
        </title>
    </border>
    <border>
        <type>amp</type>
        <position>left</position>
        <titleFont>
            <size>12</size>
            <name>MS-Trebuchet</name>
        </titleFont>
        <title>
            <text>Amplitude</text>
            <font>
                <size>18</size>
                <name>MS-Trebuchet</name>
                <style>bold</style>
            </font>
        </title>
    </border>
    <border>
        <type>title</type>
        <position>right</position>
        <title>
            <text>Vertical</text>
            <font>
                <size>18</size>
                <name>MS-Trebuchet</name>
                <style>bold</style>
            </font>
        </title>
    </border>
</seismogramDisplay>
