eventattr.create=CREATE TABLE eventattr (eventid int primary key, \
                                              eventname varchar, \
                                              eventflinnengdahlid int REFERENCES \
                                              flinnengdahl, \
                                              preferredorigin int)

eventparameterreference.create=CREATE TABLE eventparameterreference ( \
                                    eventparameterreferenceid int REFERENCES \
                                    eventattr, \
                                    parameterid int REFERENCES \
                                    parameterref )
unit.create=CREATE TABLE unit ( dbid int primary key, \
                                name text, \
                                base varchar(10), \
                                subunit_dbid text, \
                                exponent int, \
                                multFactor float8, \
                                power int )
                                
location.create=CREATE TABLE location (locationid int primary key, \
                                       locationlatitude float, \
                                       locationlongitude float, \
                                       locationelevationvalue float, \
                                       locationelevationunitid int REFERENCES \
                                       unit, \
                                       locationdepthvalue float, \
                                       locationdepthunitid int REFERENCES \
                                       unit, \
                                       locationtype int CHECK(locationtype>=0 AND locationtype<=1))
                                       
contributor.create=CREATE TABLE contributor (contributorid int primary key, \
                                             contributor text )
                                             
magnitude.create=CREATE TABLE magnitude ( magnitudeid int primary key, \
                                          magnitudetype  text, \
                                          magnitudevalue float, \
                                          magnitudecontributorid int  )
                                          
catalog.create=CREATE TABLE catalog (catalogid int  primary key, \
                                     catalogcontributorid int REFERENCES \
                                     contributor unique, \
                                     catalog text)
                                     
origin.create=CREATE TABLE origin (originid int primary key, \
                                   origincatalogid int, \
                                   origineventid int REFERENCES eventattr, \
                                   origin_time timestamp, originnanoseconds int, \
                                   originleapseconds int, \
                                   originlocationid int REFERENCES location, \
                                   origintextid varchar)
                                   
originparamref.create=CREATE TABLE originparamref (originparamrefid int REFERENCES origin, \
                                                   originparameterid int REFERENCES  parameterref)
                                                   
originmagnitude.create=CREATE TABLE originmagnitude (originmagnitudeid int REFERENCES origin, \
                                                     originrefmagnitudeid int REFERENCES magnitude)
