#Default queries for the anhinga database calls
eventattr.create=CREATE TABLE eventattr ( eventid int, \
                                          eventname varchar, \
                                          eventflinnengdahlid int,  \
                                          preferredorigin int, \
                                          FOREIGN KEY ( eventflinnengdahlid ) \
                                          REFERENCES flinnengdahl ( flinnengdahlid ), \
                                          PRIMARY KEY (eventid))
                                                                                              
eventparameterreference.create=CREATE TABLE eventparameterreference ( \
                                    eventparameterreferenceid int, \
                                    parameterid int , \
                                    FOREIGN KEY (eventparameterreferenceid ) \
                                    REFERENCES eventattr ( eventid ), \
                                    FOREIGN KEY (parameterid ) \
                                    REFERENCES parameterref ( parameterid ))
                                    
unit.create=CREATE TABLE unit ( dbid int, \
                                name varchar, \
                                base varchar(10), \
                                subunit_dbid varchar, \
                                exponent int, \
                                multFactor double, \
                                power int, \
                                PRIMARY KEY (dbid) )
                                
location.create=CREATE TABLE location (locationid int, \
                                       locationlatitude float, \
                                       locationlongitude float, \
                                       locationelevationvalue float, \
                                       locationelevationunitid int, \
                                       locationdepthvalue float, \
                                       locationdepthunitid int, \
                                       locationtype int, \
                                       PRIMARY KEY (locationid), \
                                       FOREIGN KEY (locationelevationunitid) \
                                       REFERENCES unit (dbid), \
                                       FOREIGN KEY (locationdepthunitid) \
                                       REFERENCES unit (dbid))
                                       
contributor.create=CREATE TABLE contributor (contributorid int, \
                                             contributor varchar, \
                                             PRIMARY KEY (contributorid) )
                                             
magnitude.create=CREATE TABLE magnitude ( magnitudeid int, \
                                          magnitudetype varchar, \
                                          magnitudevalue float, \
                                          magnitudecontributorid int, \
                                          PRIMARY KEY (magnitudeid) )
                                          
catalog.create=CREATE TABLE catalog (catalogid int, \
                                     catalogcontributorid int, \
                                     catalog varchar, \
                                     PRIMARY KEY (catalogid), \
                                     FOREIGN KEY (catalogcontributorid ) \
                                     REFERENCES contributor (contributorid), \
                                     UNIQUE (catalogcontributorid))
                                     
origin.create=CREATE TABLE origin (originid int, origincatalogid int, \
                                   origineventid int, origin_time timestamp, \
                                   originnanoseconds int, originleapseconds int, \
                                   originlocationid int, \
                                   origintextid varchar, \
                                   PRIMARY KEY (originid), \
                                   FOREIGN KEY (originlocationid) \
                                   REFERENCES location (locationid))
                                   
originparamref.create=CREATE TABLE originparamref (originparamrefid int, \
                                                   originparameterid int, \
                                                   FOREIGN KEY (originparamrefid) \
                                                   REFERENCES origin (originid), \
                                                   FOREIGN KEY (originparameterid) \
                                                   REFERENCES parameterref (parameterid))
                                   
originmagnitude.create=CREATE TABLE originmagnitude (originmagnitudeid int, \
                                                     originrefmagnitudeid int, \
                                                     FOREIGN KEY (originmagnitudeid) \
                                                     REFERENCES origin (originid), \
                                                     FOREIGN KEY (originrefmagnitudeid) \
                                                     REFERENCES magnitude (magnitudeid))
                                                     
EventAccess.create=CREATE TABLE eventaccess (eventid int, IOR varchar, originid int, \
                                             eventattrid int, server varchar, \
                                             dns varchar, PRIMARY KEY (eventid))
                                    

EventAttrSeq.create=CREATE SEQUENCE EventAttrSeq
EventAttrSeq.nextVal=SELECT NEXTVAL ('EventAttrSeq')

FlinnEngdahlSeq.create=CREATE SEQUENCE FlinnEngdahlSeq
FlinnEngdahlSeq.nextVal=SELECT NEXTVAL ('FlinnEngdahlSeq')

ParameterRefSeq.create=CREATE SEQUENCE ParameterRefSeq
ParameterRefSeq.nextVal=SELECT NEXTVAL ('ParameterRefSeq')

unit_seq.create=CREATE SEQUENCE unit_seq
unit_seq.nextVal=SELECT NEXTVAL ('unit_seq')

LocationSeq.create=CREATE SEQUENCE LocationSeq
LocationSeq.nextVal=SELECT NEXTVAL ('LocationSeq')

ContributorSeq.create=CREATE SEQUENCE ContributorSeq
ContributorSeq.nextVal=SELECT NEXTVAL ('ContributorSeq')

MagnitudeSeq.create=CREATE SEQUENCE MagnitudeSeq
MagnitudeSeq.nextVal=SELECT NEXTVAL ('MagnitudeSeq')

CatalogSeq.create=CREATE SEQUENCE CatalogSeq
CatalogSeq.nextVal=SELECT NEXTVAL ('CatalogSeq')

OriginSeq.create=CREATE SEQUENCE OriginSeq
OriginSeq.nextVal=SELECT NEXTVAL ('OriginSeq')

EventAccessSeq.create=CREATE SEQUENCE EventAccessSeq
EventAccessSeq.nextVal=SELECT NEXTVAL ('EventAccessSeq')
