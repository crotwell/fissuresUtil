<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="edu.sc.seis.fissuresUtil.database.plottable.PlottableChunk"
		table="plottable">
		<id name="dbid" type="long">
			<generator class="native" />
		</id>
		<property name="networkCode" />
		<property name="stationCode"/>
		<property name="siteCode" />
		<property name="channelCode"/>
		<property name="beginTimestamp" type="timestamp"/>
		<property name="endTimestamp" type="timestamp"/>
		<property name="pixelsPerDay" type="integer" />
		<property name="beginPixel" type="integer" />
		<component name="data"
			class="edu.iris.Fissures.Plottable"  lazy="false">
			<property name="x_coor" type="primitive-array" access="field"  lazy="false"/>
			<property name="y_coor" type="primitive-array" access="field"  lazy="false"/>
		</component>
	</class>

</hibernate-mapping>